<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/admin_nav_graph.xml"
    app:startDestination="@id/allArticlesFragment">

    <fragment
        android:id="@+id/createArticleFragment2"
        android:name="com.mrwhoknows.csgeeks.main_ui.create.CreateArticleFragment"
        android:label="Create Article" >
        <action
            android:id="@+id/action_createArticleFragment2_to_createArticleBodyFragment"
            app:destination="@id/createArticleBodyFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/allArticlesFragment"
        android:name="com.mrwhoknows.csgeeks.admin_ui.AllArticlesFragment"
        android:label="All Articles" >
        <action
            android:id="@+id/action_allArticlesFragment_to_articleFragment2"
            app:destination="@id/articleFragment2"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/articleFragment2"
        android:name="com.mrwhoknows.csgeeks.main_ui.article.ArticleFragment"
        android:label="ArticleFragment" >
        <argument
            android:name="articleID"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/createArticleBodyFragment"
        android:name="com.mrwhoknows.csgeeks.main_ui.create.CreateArticleBodyFragment"
        android:label="Create Article" >
        <argument
            android:name="title"
            app:argType="string" />
        <argument
            android:name="description"
            app:argType="string" />
        <argument
            android:name="authorName"
            app:argType="string" />
        <argument
            android:name="thumbImgLink"
            app:argType="string" />
        <argument
            android:name="tags"
            app:argType="string" />
        <action
            android:id="@+id/action_createArticleBodyFragment_to_allArticlesFragment"
            app:destination="@id/allArticlesFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/allArticlesFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>